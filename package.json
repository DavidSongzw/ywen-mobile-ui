{
  "name": "rc-ywen-mobile-ui",
  "version": "1.0.0",
  "description": "ywen-mobile-ui ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-ywen-mobile-ui",
    "mobile",
    "ywen-mobile-ui"
  ],
  "homepage": "https://github.com/yaliyingwy/ywen-mobile-ui",
  "author": "yaliyingwy@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yaliyingwy/ywen-mobile-ui.git"
  },
  "bugs": {
    "url": "https://github.com/yaliyingwy/ywen-mobile-ui/issues"
  },
  "files": [
    "lib",
    "assets/*"
  ],
  "licenses": "MIT",
  "main": "./lib/index",
  "config": {
    "port": 8000
  },
  "scripts": {
    "build": "node_modules/rc-tools/bin/rc-tools.js run build",
    "compile": "node_modules/rc-tools/bin/rc-tools.js run compile",
    "gh-pages": "node_modules/rc-tools/bin/rc-tools.js run gh-pages",
    "start": "rc-server",
    "pub": "node_modules/rc-tools/bin/rc-tools.js run pub",
    "lint": "node_modules/rc-tools/bin/rc-tools.js run lint",
    "watch": "node_modules/rc-tools/bin/rc-tools.js run watch",
    "karma": "node_modules/rc-tools/bin/rc-tools.js run karma",
    "saucelabs": "node_modules/rc-tools/bin/rc-tools.js run saucelabs",
    "my-saucelabs": "node_modules/rc-tools/bin/rc-tools.js run my-saucelabs",
    "browser-test": "node_modules/rc-tools/bin/rc-tools.js run browser-test",
    "browser-test-cover": "node_modules/rc-tools/bin/rc-tools.js run browser-test-cover",
    "clean": "node_modules/rc-tools/bin/rc-tools.js run clean"
  },
  "devDependencies": {
    "expect.js": "0.3.x",
    "pre-commit": "1.x",
    "rc-server": "3.x",
    "rc-tools": "4.x",
    "react": "0.14.x",
    "react-addons-test-utils": "0.14.x",
    "react-dom": "0.14.x"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "react-addons-css-transition-group": "^0.14.7"
